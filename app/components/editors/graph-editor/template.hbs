<svg class="row stretch" xmlns="http://www.w3.org/2000/svg">
  <g>
    {{#each model.childEdges as |edge|}}
      {{edges/default-edge model=edge}}
    {{/each}}
  </g>
  <g>
    {{#each model.childNodes as |node|}}
      {{component node.nodeType
          model=node
          onClick=(action focusNode node)
          onDrag=(action onNodeDrag node)
          onDrop=(action onNodeDrop node)}}
    {{/each}}
  </g>
</svg>
